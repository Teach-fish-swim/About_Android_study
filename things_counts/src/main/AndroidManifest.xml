<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.things_counts">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/count_things_Theme">
        <activity
                android:theme="@style/count_things_Theme"
                android:name=".MainActivity"
                android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".HistoryEvent" android:label="历史事件"></activity>
        <activity android:name=".CreateEvent" android:label="创建事件"></activity>
        <activity android:name=".DetailPage" android:label="事件详情"/>
        <activity android:name=".RemoveEvent" android:label="删除或修改事件"></activity>
        <activity android:name=".LookHistoryCount"
                  android:theme="@style/count_things_Theme"
                  android:label="查看历史账单"></activity>
    </application>

</manifest>